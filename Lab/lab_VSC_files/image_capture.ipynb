    "import cv2\n",
    "\n",
    "def read_and_write_from_camera():\n",
    "    cap = cv2.VideoCapture(0)\n",
    "    ret, frame = cap.read()\n",
    "    if ret:\n",
    "        cv2.imwrite(\"captured_image.jpg\", frame)\n",
    "    cap.release()\n",
    "    cv2.destroyAllWindows()"
   ]
